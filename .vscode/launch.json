{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run TechEd2020-SensorManager",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/teched2020-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/keepcoolSensorManager/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/SensorManager",
      "env": {
        "PORT": "6006",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/TechEd2020/SensorManager\"}"
      },
      "envFile": "${workspaceFolder}/SensorManager/.env2",
      "preLaunchTask": "Build TechEd2020"
    },
    {
      "name": "Run TechEd2020_B2C",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/teched2020-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/keepcoolSensorManager/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/SensorManager",
      "env": {
        "PORT": "6007",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/TechEd2020/SensorManager\"}"
      },
      "envFile": "${workspaceFolder}/SensorManager/.env3",
      "preLaunchTask": "Build TechEd2020"
    }
  ]
}